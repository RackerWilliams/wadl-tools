         <!-- Inlcude Public API -->
    <!--Source: file:/home/dcramer/Documents/rax/authWadl/resources/xsd/api.xsd--><annotation>
            <xsd:appinfo xmlns="http://www.w3.org/1999/xhtml" xml:lang="EN">
               <xsdxt:title>Auth API Schema Types</xsdxt:title>
